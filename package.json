  "license": "MIT",
  "repository": "wladimirguerra/react-common-mui-components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "shx rm -Rf dist/*",
    "build": "yarn run clean && tsc",
